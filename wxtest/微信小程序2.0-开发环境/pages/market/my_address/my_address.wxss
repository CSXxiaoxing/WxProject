page {
  padding-bottom: 120rpx;
  background: #f1f0fa;
  min-height: 1130rpx;
}

.add_address {
  font-size: 30rpx; 
  position: fixed;
  bottom: 0;
  line-height: 100rpx;
  color: #91a3ff;
  text-align: center;
  width: 100%;
  background-color: #fff;
  
  border: 1rpx solid #333;
  box-sizing: border-box;
}
/*marginLeft*/
.marginLeft{
  margin-left: 50rpx;
}
/*地址子项*/

.address_item {
  position: relative;
  margin-top: 20rpx;
  background-color: #fff;
  height: 173rpx;
  display: flex;
  align-items: center;
  line-height: 30rpx;
}
.selIcon-container{
    padding: 66rpx 30rpx 66rpx 30rpx;
}
.selIcon-container image {
  display: block;
  width: 40rpx;
  height: 40rpx;

}

.user_desc {
  width: 350rpx;
  font-size: 28rpx;
}

.user_info {
  margin-bottom: 22rpx;
}
.btns{
    position: absolute;
  color: #91a3ff;
  right: 0;
   font-size: 30rpx;
   display: flex;
   line-height: 173rpx;
}
.btns::after{
  content: '';
  background: #e6e6e6;
  width: 1px;
  height: 50;
}
.sel_btn{
  padding: 0 20rpx 0 51rpx;
}
.edit_btn{
 padding:0 51rpx 0 20rpx;
}

.phone {
  font-size: 30rpx;
}
.address_info{
  width: 100%;
  height: 30rpx;
    overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  color: #808080
}
