<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="warn" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">提现失败</view>
            <view class="weui-msg__desc">{{message}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" style="background-color: #2196f3;font-size:30rpx;line-height:88rpx;" open-type="contact">联系客服</button>
                <navigator open-type="navigateBack" delta="2" class="back" type="default">返回</navigator>
            </view>
        </view>
    </view>
</view>